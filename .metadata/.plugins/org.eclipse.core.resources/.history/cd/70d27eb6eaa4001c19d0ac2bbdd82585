package day01;

public class Ex02 {
 public static void main(String[] args) {
	System.out.println(100+100);
	System.out.println(100.123+100);
	
	System.out.println("백"+100+"\n"+100);
	System.out.println("100"+100);
	
	
	System.out.println("이름 \t나이 \t전화번호 \t회비");
	System.out.println("홍길동"+"15");
	
	
} 
}
